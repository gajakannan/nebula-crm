{
  "realm": "nebula",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": false,
  "accessTokenLifespan": 300,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "roles": {
    "realm": [
      { "name": "DistributionUser", "description": "Distribution team member — owns brokers, contacts, submissions" },
      { "name": "DistributionManager", "description": "Distribution team lead — manages team members and escalations" },
      { "name": "Underwriter", "description": "Underwriting team member — reviews submissions, sets terms" },
      { "name": "RelationshipManager", "description": "Relationship management — oversees key broker relationships" },
      { "name": "ProgramManager", "description": "Program management — manages programs and renewals" },
      { "name": "Admin", "description": "System administrator — full access" }
    ]
  },
  "clients": [
    {
      "clientId": "nebula-crm",
      "name": "Nebula CRM SPA",
      "description": "React SPA front-end (public client with PKCE)",
      "enabled": true,
      "publicClient": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "implicitFlowEnabled": false,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "rootUrl": "http://localhost:5173",
      "baseUrl": "/",
      "redirectUris": [
        "http://localhost:5173/*",
        "http://localhost:8080/*"
      ],
      "webOrigins": [
        "http://localhost:5173",
        "http://localhost:8080"
      ],
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:5173/*"
      },
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "profile",
        "roles",
        "email"
      ]
    },
    {
      "clientId": "nebula-api",
      "name": "Nebula API",
      "description": "Backend API — bearer-only client for audience validation",
      "enabled": true,
      "publicClient": false,
      "bearerOnly": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "implicitFlowEnabled": false,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect"
    }
  ],
